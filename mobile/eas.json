{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://pantry.up.railway.app"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6755445323",
        "appleTeamId": "K5A25879TB"
      }
    }
  }
}

